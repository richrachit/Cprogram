#include<stdio.h>
int main()
{
int a[10],i,n,p,x;
printf("Enter n:");
scanf("%d"&n);
printf("enter array element:\n");
for(i=1;i<=n;i++)
 scanf("%d",& a[i]);
 printf("Enter element to be inserted is x:");
 scanf("%d",&x);
 for(i=n;i>=p;i--)
 {
 a(i+1) = a[i];
 }
 a[p] = x;
 n=n+1;
 printf("final array is \n");
 for(i=0;i<=n;i++)
 printf("%d\n",a[i]);
 printf("No. of element in array after insert = %d",n);
 return 0;
 }
